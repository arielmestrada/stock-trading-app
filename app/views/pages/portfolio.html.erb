<h1>Portfolio Page</h1>

<div>
    <% @stocks.each do |stock| %>
        <p><%= stock.listing.name %></p>
        <p>Quantity you have: <%= stock.quantity %></p>
        <%= link_to "Buy", buy_stock_path(stock.listing.id) %><br/>
        <%= link_to "Sell", sell_stock_path(stock.listing.id) %>
    <% end %>
</div>